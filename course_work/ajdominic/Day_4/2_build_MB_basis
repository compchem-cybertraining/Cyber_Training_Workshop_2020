    "S_ci, St_ci  = [], []\n",
    "for step in range( nsteps ):\n",
    "    s_ci  = SD2CI[step].H() * S_sd[step]  * SD2CI[step]\n",
    "    S_ci.append(  s_ci  )\n",
    "for step in range( nsteps-1 ):\n",
    "    st_ci = SD2CI[step].H() * St_sd[step] * SD2CI[step+1]\n",
    "    St_ci.append( st_ci )"
