    "\n",
    "params[\"outdir\"] = res_dir\n",
    "SD2CI = step3_many_body.make_T_matrices( ci_coefficients, ci_basis_states, \n",
    "                                spin_components, sd_states_unique_sorted, \n",
    "                                params )"
